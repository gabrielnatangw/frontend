# Configuração para Staging (Branch develop)
VITE_API_URL=https://smart-platform.io:8443/api-v2
VITE_WS_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_INTERVAL=3000
VITE_WS_HEARTBEAT_INTERVAL=30000

# App Configuration
VITE_APP_NAME=SMART TRACE
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=staging

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=true
VITE_DEBUG=true

# New Relic Configuration
VITE_NEW_RELIC_ENABLED=true
VITE_NEW_RELIC_LICENSE_KEY=your_license_key_here
VITE_NEW_RELIC_APPLICATION_ID=your_app_id_here
VITE_NEW_RELIC_ACCOUNT_ID=your_account_id_here
VITE_NEW_RELIC_TRUST_KEY=your_trust_key_here